while wait() do
   pcall(function()
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Iron Sword"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Steel Sword"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Platinum Sword"])
wait(2)
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Flamberge"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Poison Flamberge"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Bandit Slayer"])
wait(2)
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Demonic Slayer"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Holy Slayer"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Liberator"])
wait(2)
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Blessed Liberator"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Khopesh"])
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Refined Khopesh"])
wait(1)
game:GetService("ReplicatedStorage").Remotes.FuseSword:FireServer(game:GetService("Players").LocalPlayer.PlayerFolder.Swords["Flamberge"])
end)
end
